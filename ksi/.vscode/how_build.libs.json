{
	"win64:release" : {
		"_prepare" : {
			"dir": "${workspace_dir}/obj",
			"command": "g++.exe",
			"args": [
				"-std=c++20",
				"-fmodules-ts",
				"-xc++-system-header",
				"-m64",
				"${each}"
			],
			"each_plain" : [
				"initializer_list",
				"type_traits",
				"concepts",
				"compare",
				"utility",
				"string_view",
				"iostream",
				"cstring",
				"cwchar"
			]
		}
	}
}