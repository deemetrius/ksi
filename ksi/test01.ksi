
&test_for range n ~
	range #dump #0
	" for\n" #echo #
	`while: #1 ~
		range `for i : v \
			i ? `break=2 ;
		~
			i == 0 ?
				[i " skip\n"] #echo #
				`next
			;
			{%index: i, %value: v} #echo [', ' ": "]
			"\n" #echo #
		;
	;
;

`plain -- привет

[1_ 1.4 0.5] &test_for #
[1 1.4_ 0.5] &test_for #
